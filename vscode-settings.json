{
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.detectIndentation": false,
	"editor.formatOnSave": true,
	"editor.insertSpaces": false,
	"editor.renderWhitespace": "all",
	"editor.suggestSelection": "first",
	"files.trimTrailingWhitespace": true,
	"git.autofetch": true,
	"window.autoDetectColorScheme": true,
	// "workbench.colorTheme": "Default Dark+",
	"workbench.startupEditor": "welcomePage",
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	// Extension settings
	// // Docs Authoring Pack Begin // //
	"docs-build.userType": "Public Contributor",
	"markdownlint.config": {
		"MD028": false,
		"MD025": {
			"front_matter_title": ""
		}
	},
	"markdownlint.customRules": [
		"{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
	],
	// // Docs Authoring Pack End // //
	"mssql.format.datatypeCasing": "uppercase",
	"mssql.format.keywordCasing": "uppercase",
	"mssql.persistQueryResultTabs": true,
	"mssql.showBatchTime": true,
	"prettier.useTabs": true,
	"redhat.telemetry.enabled": true,
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"xml.symbols.maxItemsComputed": 10000,
	// SQL Server connections
	"mssql.connections": [
		{
			"server": "{{put-server-name-here}}",
			"database": "{{put-database-name-here}}",
			"user": "{{put-username-here}}",
			"password": ""
		}
	]
}
